            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.IdInventarioNavigation)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.IdInventarioNavigation.Id)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.IdPacienteNavigation)
